<p id="notice"><%= notice %></p>

<h1>Judges</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Issitting</th>
      <th>Biography</th>
      <th>Email</th>
      <th>Pol aff</th>
      <th>Ballot</th>
      <th>Precinct</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @judges.each do |judge| %>
      <tr>
        <td><%= judge.first_name %></td>
        <td><%= judge.last_name %></td>
        <td><%= judge.isSitting %></td>
        <td><%= judge.biography %></td>
        <td><%= judge.email %></td>
        <td><%= judge.pol_Aff %></td>
        <td><%= judge.ballot %></td>
        <td><%= judge.precinct %></td>
        <td><%= link_to 'Show', judge %></td>
        <td><%= link_to 'Edit', edit_judge_path(judge) %></td>
        <td><%= link_to 'Destroy', judge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Judge', new_judge_path %>
