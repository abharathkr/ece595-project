<h1>Candidates</h1>
<p>Select a candidate&hellip;</p>
<table class="detail-table candidate-table">
    <thead>
        <tr>
            <th>Last Name</th>
            <th>First Name</th>
            <th>Party</th>
            <th>Encumbent</th>
            <th>Precinct</th>
            <th>E-Mail</th>
            <th>Website</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
      <% @candidates.each do |candidate| %>
        <tr>
            <td><%= candidate.last_name %></td>
            <td><%= candidate.first_name %></td>
            <td><%= candidate.pol_Aff %></td>
            <td><%= candidate.isEncumbent %></td>
            <td><%= candidate.precinct_id %></td>
            <td><a href="mailto:<%= candidate.email %>"><%= candidate.email %></a></td>
            <td><a href="#">Link</a></td>
            <td><%= link_to 'More...', candidate %></td>
        </tr>
    <% end %>
    </tbody>
</table>
